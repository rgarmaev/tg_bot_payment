# Telegram bot
TELEGRAM_BOT_TOKEN=CHANGE_ME
ADMIN_USER_ID=

# Public base URL of this app (scheme + host), used for links shown to users
# Example: https://vpn.example.com
PUBLIC_BASE_URL=

# Payment provider
PAYMENT_PROVIDER=robokassa

# Robokassa settings
ROBOKASSA_LOGIN=
ROBOKASSA_PASSWORD1=
ROBOKASSA_PASSWORD2=
ROBOKASSA_IS_TEST=true
ROBOKASSA_CULTURE=ru

# x3-ui panel
X3UI_BASE_URL=http://127.0.0.1:2053
X3UI_USERNAME=
X3UI_PASSWORD=
X3UI_API_KEY=
X3UI_INBOUND_ID=1
X3UI_CLIENT_DAYS=30
# Leave empty for unlimited traffic, or set integer GB
X3UI_CLIENT_TRAFFIC_GB=

# Tariff
PLAN_NAME=Monthly
PLAN_DAYS=30
PLAN_PRICE_RUB=300

# Database (SQLite by default)
SQLITE_URL=sqlite+aiosqlite:///./bot.db
